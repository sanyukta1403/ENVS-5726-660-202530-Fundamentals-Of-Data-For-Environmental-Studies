def is_min_ratio_toilets_to_people_met(ratio):
    ratio_parts= ratio.split('/')
    toilet_part = ratio_parts[0]
    people_part = ratio_parts [1]

    toilets = int(toilet_part.replace("t",""))
    people = int(people_part.replace("p",""))

    ratio_value = toilets/people

    if ratio_value >= 1/20:
        return True
    else:
        return False
    
#print(is_min_ratio_toilets_to_people_met('1t/37p'))
#print(is_min_ratio_toilets_to_people_met('1t/12p'))
    


def is_population_disabled(disabled:float, total_population:float):
    if disabled/total_population < 0.1:
        return False
    else:
        return True
#print(is_population_disabled(disabled= 0, total_population=32))
#print(is_population_disabled(disabled=52, total_population=392))



def is_gp_religious_or_academic(gp):
    keywords = {'Mosque', 'Church', 'School', 'Institute', 'Education', 'Faculty'}  # already a set

    # Split gp string into words
    gp_words = set(gp.split(' ')) 

    # Find intersection
    intersection_a_keywords = gp_words.intersection(keywords)

    if intersection_a_keywords: #this means if this object exists
        return True
    else:
        return False #can also write as return bool(intersection_a_keywords)


print(is_gp_religious_or_academic(gp='Faculty of Earth Sciences and Mining')) # what's in the () in def is in the () in print 
print(is_gp_religious_or_academic(gp='Almorada Church')) #so it calls to the same thing that's how gp could split without typ[ing all the words
print(is_gp_religious_or_academic('Health Insulation Building'))



def get_sanitation_priority(ratio, disabled, total_population, gp):
    ratio = is_min_ratio_toilets_to_people_met(ratio)
    disabled = is_population_disabled(disabled, total_population)
    gp = is_gp_religious_or_academic(gp)

    if not ratio and disabled and gp:
        return 'High Priority'
    elif ratio and not disabled and not gp:
        return 'Low Priority'
    else:
        return 'Medium Priority'

